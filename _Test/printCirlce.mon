
let main = fn (){
    let dimension = 40;
    let matrix = []
    let radius = dimension / 2

    for(let i = 0; i < dimension; i = i + 1){
        let tempArray = []
        for(let j = 0; j < dimension; j = j + 1){
            if( (i-radius)*(i-radius)+(j-radius)*(j-radius) < radius*radius ){
                print("* ");
            }
            else {
                print("  ");
            }
        }
        print("\n")
    }
}

main()