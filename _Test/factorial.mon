let factorial = fn(x){
    if (x < 0) {
        print("Argument of factorial negative!")
        return 0;
    }
    if (x < 2) {
        return 1;
    }
    else {
        return x * factorial(x - 1);
    }
}

let computed = [];

let fillArray = fn(n){
    if (n > 0) {
        fillArray(n-1);
    }
    push(computed,factorial(n));
}

fillArray(10);

computed